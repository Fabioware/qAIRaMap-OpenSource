{
	"name": "qairamapos",
	"version": "1.0.0",
	"description": "Real-time website for monitoring air quality",
	"main": "index.js",
	"scripts": {
		"test": "jest --coverage ",
		"test:watch": "jest --watch",
		"test:coveralls": "jest --coverage && cat ./tests/coverage/lcov.info | coveralls",
		"serve": "webpack-dev-server -p",
		"webpack": "webpack -d --watch",
		"start": "npm run webpack",
		"doc": "jsdoc -c jsdoc.json"
	},
	"jest": {
		"verbose": true,
		"moduleDirectories": [
			"node_modules",
			"src"
		],
		"moduleFileExtensions": [
			"js",
			"jsx",
			"json",
			"css",
			"scss"
		],
		"transform": {
			"^.+\\.jsx?$": "babel-jest"
		},
		"moduleNameMapper": {
			"\\.(css|less|sass|scss)$": "identity-obj-proxy"
		},
		"automock": false,
		"setupFiles": [
			"./setupJest.js"
		],
		"coveragePathIgnorePatterns": [
			"./build/"
		],
		"coverageDirectory": "./tests/coverage",
		"collectCoverage": true,
		"coverageReporters": ["html", "text-summary", "lcov"]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/qAIRa/qAIRaMap-OpenSource.git"
	},
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/qAIRa/qAIRaMap-OpenSource/issues"
	},
	"homepage": "https://github.com/qAIRa/qAIRaMap-OpenSource#readme",
	"dependencies": {
		"@babel/runtime": "^7.11.2",
		"@googlemaps/jest-mocks": "0.0.3",
		"babel-jest": "^25.3.0",
		"webpack": "^4.41.6",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.11.0"
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-transform-modules-commonjs": "^7.9.0",
		"@babel/plugin-transform-runtime": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"babel-core": "^6.26.3",
		"coveralls": "^3.1.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^25.5.4",
		"jest-cli": "^26.4.2",
		"jest-fetch-mock": "^3.0.3",
		"jsdoc": "^3.6.4"
	}
}
